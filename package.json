{
  "name": "mutual-api-server",
  "version": "1.0.0",
  "description": "The app server api for the mutual app.",
  "main": "master.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "dev": "nodemon | pino-pretty",
    "start": "node build/master.js",
    "deploy": "npm run start",
    "migrate": "knex migrate:latest --knexfile knexfile.js",
    "seed": "knex seed:run --knexfile knexfile.js",
    "test": "tap && standard --fix",
    "clean": "rimraf build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "postinstall": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit"
  },
