    "@fastify/swagger": "^6.1.0",
    "aws-sdk": "^2.1099.0",
    "config": "^3.3.7",
    "fastify": "^3.27.1",
    "fastify-formbody": "^5.3.0",
    "fastify-jwt": "^4.1.3",
    "fastify-multer": "^2.0.2",
    "forky": "^1.2.0",
    "google-libphonenumber": "^3.2.27",
    "googleapis": "^107.0.0",
    "json-schema-faker": "^0.5.0-rcv.44",
    "knex": "^1.0.3",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
