{
  "name": "mutual-api-server",
  "version": "1.0.0",
  "description": "The app server api for the mutual app.",
  "main": "master.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc && tsc-alias -p tsconfig.json",
    "watch:ts": "tsc -w",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P build/src/app.js",
    "start": "fastify start -l info -s process.env.PORT -a \"::\" build/src/app.js",
    "deploy": "npm run start",
    "migrate": "npx prisma migrate",
    "seed": "knex seed:run --knexfile knexfile.js",
    "routes": "fastify print-routes ./build/src/app.js",
    "test": "tap && standard --fix",
    "clean": "rimraf build",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "postinstall": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit"
  },
  "author": "Great Okonkwo",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.2.1",
    "@fastify/autoload": "^5.7.1",
    "@fastify/compress": "^6.4.0",
    "@fastify/jwt": "^6.3.2",
    "@fastify/sensible": "^5.2.0",
    "@fastify/swagger": "^6.1.0",
    "@prisma/client": "^5.2.0",
    "aws-sdk": "^2.1099.0",
    "concurrently": "^8.2.1",
    "config": "^3.3.7",
    "dayjs": "^1.11.7",
    "fastify": "^4.15.0",
    "fastify-cli": "^5.8.0",
    "fastify-formbody": "^5.3.0",
    "fastify-jwt": "^4.1.3",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "^4.5.1",
    "fastify-zod": "^1.4.0",
    "firebase-admin": "^11.5.0",
    "forky": "^1.2.0",
    "google-libphonenumber": "^3.2.27",
    "googleapis": "^107.0.0",
    "json-schema-faker": "^0.5.0-rcv.44",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "neo4j-driver": "^4.4.7",
    "newrelic": "^8.17.0",
    "node-fetch": "^2.6.7",
    "node-pushnotifications": "^2.0.3",
    "pg": "^8.7.3",
    "phone": "^3.1.12",
    "pino": "^8.7.0",
    "redis": "4.6.8",
    "rimraf": "^3.0.2",
    "serialize-error": "^7.0.1",
    "spotify-web-api-node": "^5.0.2",
    "twilio": "^3.74.0",
    "typescript": "^5.2.2",
    "useragent": "^2.3.0",
    "uuid": "^9.0.0",
    "zod": "^3.20.2",
    "zod-to-json-schema": "^3.21.4"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.7.15",
    "@types/node-fetch": "^2.6.2",
    "@types/useragent": "^2.3.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "fastify-tsconfig": "^1.0.1",
    "json-schema-to-ts": "^2.5.5",
    "nodemon": "^2.0.15",
    "pino-pretty": "^9.1.1",
    "prettier": "2.7.1",
    "prisma": "^5.2.0",
    "standard": "^16.0.4",
    "tap": "^16.0.1",
    "tsc-alias": "^1.8.7"
  }
}
